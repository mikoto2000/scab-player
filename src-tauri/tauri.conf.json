{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "assets/*"
    ],
    "externalBin": [],
    "copyright": "mikoto2000@gmail.com",
    "category": "Music",
    "shortDescription": "Self create audio book player.",
    "longDescription": "Self create audio book player.",
    "macOS": {
      "frameworks": [],
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null,
      "entitlements": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build",
    "beforeDevCommand": "npm run start",
    "devUrl": "http://localhost:3000"
  },
  "productName": "scabplayer",
  "version": "0.1.0",
  "identifier": "dev.mikoto2000.scabplayer",
  "plugins": {},
  "app": {
    "security": {
      "csp": "default-src 'self'; connect-src ipc: http://ipc.localhost; media-src stream: http://stream.localhost asset: http://asset.localhost",
      "assetProtocol": {
        "scope": ["**/*"]
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "ScabPlayer",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  }
}